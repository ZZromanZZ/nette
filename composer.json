{
	"name": "nette/nette",
	"type": "library",
	"description": "Nette Framework - innovative framework for fast and easy development of secured web applications in PHP. Write less, have cleaner code and your work will bring you joy.",
	"keywords": ["framework", "mvc", "debugging", "templating", "database", "forms", "mailing"],
	"homepage": "http://nette.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "http://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "http://nette.org/contributors"
		}
	],
	"require": {
		"php": ">=5.3.1",
		"nette/application": "@dev",
		"nette/bootstrap": "@dev",
		"nette/caching": "@dev",
		"nette/component-model": "@dev",
		"nette/database": "@dev",
		"nette/di": "@dev",
		"nette/finder": "@dev",
		"nette/forms": "@dev",
		"nette/http": "@dev",
		"nette/mail": "@dev",
		"nette/neon": "@dev",
		"nette/php-generator": "@dev",
		"nette/reflection": "@dev",
		"nette/robot-loader": "@dev",
		"nette/safe-stream": "@dev",
		"nette/security": "@dev",
		"nette/tokenizer": "@dev",
		"nette/utils": "@dev",
		"latte/latte": "@dev",
		"tracy/tracy": "@dev",
		"ext-iconv": "*",
		"ext-tokenizer": "*"
	},
	"suggest": {
		"ext-mbstring": "*",
		"ext-pdo": "*",
		"ext-gd": "*",
		"ext-fileinfo": "*"
	},
	"require-dev": {
		"nette/tester": "~1.1"
	},
	"autoload": {
		"classmap": ["Nette/"],
		"files": ["Nette/shortcuts.php"]
	},
	"minimum-stability": "dev",
	"extra": {
		"branch-alias": {
			"dev-master": "2.3-dev"
		}
	}
}
